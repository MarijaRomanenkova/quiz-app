{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@hooks/*": ["src/hooks/*"],
      "@components/*": ["src/components/*"],
      "@types": ["src/types"],
      "@services/*": ["src/services/*"],
      "@navigator/*": ["src/navigator/*"],
      "@theme": ["src/theme"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
